# Attempt to set APP_HOME
# Resolve links: $0 may be a link
PRG="$0"
# Need this for relative symlinks.
while [ -h "$PRG" ] ; do
    ls=`ls -ld "$PRG"`
    link=`expr "$ls" : '.*-> \(.*\)$'`
    if expr "$link" : '/.*' > /dev/null; then
        PRG="$link"
    else
        PRG=`dirname "$PRG"`"/$link"
    fi
done
SAVED="`pwd`"
cd "`dirname \"$PRG\"`/.." >/dev/null
APP_HOME="`pwd -P`"
cd "$SAVED" >/dev/null

APP_NAME="evmtool"
APP_BASE_NAME=`basename "$0"`

# Add default JVM options here. You can also use JAVA_OPTS and EVMTOOL_OPTS to pass JVM options to this script.
DEFAULT_JVM_OPTS='"-Dsecp256k1.randomize=false"'

# Use the maximum available, or set MAX_FD != -1 to use that value.
MAX_FD="maximum"

warn () {
    echo "$*"
}

die () {
    echo
    echo "$*"
    echo
    exit 1
}

# OS specific support (must be 'true' or 'false').
cygwin=false
msys=false
darwin=false
nonstop=false
case "`uname`" in
  CYGWIN* )
    cygwin=true
    ;;
  Darwin* )
    darwin=true
    ;;
  MSYS* | MINGW* )
    msys=true
    ;;
  NONSTOP* )
    nonstop=true
    ;;
esac

CLASSPATH=$APP_HOME/lib/besu-24.12.2.jar:$APP_HOME/lib/besu-evmtool-24.12.2.jar:$APP_HOME/lib/besu-24.12.2.jar:$APP_HOME/lib/besu-ethereum-stratum-24.12.2.jar:$APP_HOME/lib/besu-ethereum-ethstats-24.12.2.jar:$APP_HOME/lib/besu-clique-24.12.2.jar:$APP_HOME/lib/besu-ibft-24.12.2.jar:$APP_HOME/lib/besu-qbft-24.12.2.jar:$APP_HOME/lib/besu-consensus-common-24.12.2.jar:$APP_HOME/lib/besu-api-24.12.2.jar:$APP_HOME/lib/besu-merge-24.12.2.jar:$APP_HOME/lib/besu-blockcreation-24.12.2.jar:$APP_HOME/lib/besu-plugin-rocksdb-24.12.2.jar:$APP_HOME/lib/besu-eth-24.12.2.jar:$APP_HOME/lib/besu-permissioning-24.12.2.jar:$APP_HOME/lib/besu-p2p-24.12.2.jar:$APP_HOME/lib/referencetests-24.12.2.jar:$APP_HOME/lib/besu-core-24.12.2.jar:$APP_HOME/lib/core-support-test-24.12.2-test-support.jar:$APP_HOME/lib/besu-config-24.12.2.jar:$APP_HOME/lib/besu-trie-24.12.2.jar:$APP_HOME/lib/besu-kvstore-24.12.2.jar:$APP_HOME/lib/besu-crypto-services-24.12.2.jar:$APP_HOME/lib/besu-metrics-rocksdb-24.12.2.jar:$APP_HOME/lib/besu-pipeline-24.12.2.jar:$APP_HOME/lib/besu-tasks-24.12.2.jar:$APP_HOME/lib/besu-metrics-core-24.12.2.jar:$APP_HOME/lib/plugin-api-24.12.2.jar:$APP_HOME/lib/besu-evm-24.12.2.jar:$APP_HOME/lib/besu-datatypes-24.12.2.jar:$APP_HOME/lib/enclave-24.12.2.jar:$APP_HOME/lib/besu-nat-24.12.2.jar:$APP_HOME/lib/besu-ethereum-rlp-24.12.2.jar:$APP_HOME/lib/besu-util-24.12.2.jar:$APP_HOME/lib/besu-crypto-24.12.2.jar:$APP_HOME/lib/jackson-datatype-jdk8-2.18.0.jar:$APP_HOME/lib/core-4.12.2.jar:$APP_HOME/lib/crypto-4.12.2.jar:$APP_HOME/lib/client-java-21.0.1-legacy.jar:$APP_HOME/lib/aws-java-sdk-sts-1.12.746.jar:$APP_HOME/lib/aws-java-sdk-core-1.12.746.jar:$APP_HOME/lib/jmespath-java-1.12.746.jar:$APP_HOME/lib/jackson-dataformat-cbor-2.18.0.jar:$APP_HOME/lib/jackson-databind-2.18.0.jar:$APP_HOME/lib/jackson-annotations-2.18.0.jar:$APP_HOME/lib/vertx-web-4.5.10.jar:$APP_HOME/lib/vertx-auth-jwt-4.5.10.jar:$APP_HOME/lib/vertx-unit-4.5.10.jar:$APP_HOME/lib/vertx-web-common-4.5.10.jar:$APP_HOME/lib/vertx-au